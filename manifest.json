{
  "name": "ATI",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://loads.ati.su/*"
      ],
      "js": ["content.js"]
    }
  ]
}